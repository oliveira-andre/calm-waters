<p id="notice"><%= notice %></p>

<h1>Solo Sensors</h1>

<table>
  <thead>
    <tr>
      <th>Tank</th>
      <th>Calcium</th>
      <th>Ph</th>
      <th>Phosphoror</th>
      <th>Carbon</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @solo_sensors.each do |solo_sensor| %>
      <tr>
        <td><%= solo_sensor.tank %></td>
        <td><%= solo_sensor.calcium %></td>
        <td><%= solo_sensor.ph %></td>
        <td><%= solo_sensor.phosphoror %></td>
        <td><%= solo_sensor.carbon %></td>
        <td><%= link_to 'Show', solo_sensor %></td>
        <td><%= link_to 'Edit', edit_solo_sensor_path(solo_sensor) %></td>
        <td><%= link_to 'Destroy', solo_sensor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Solo Sensor', new_solo_sensor_path %>
